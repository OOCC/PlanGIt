//wp-shareto js代码，为了提高页面加载速度 将js代码压缩

function shareToSina(title,url){var url="http://v.t.sina.com.cn/share/share.php?title="+encodeURIComponent(title)+"&url="+encodeURIComponent(url);openwindow(url);}function shareToQzone(title,url){var url="http://sns.qzone.qq.com/cgi-bin/qzshare/cgi_qzshare_onekey?title="+encodeURIComponent(title)+"&url="+encodeURIComponent(url);openwindow(url);}function shareToDigu(title,url){var url="http://www.diguff.com/diguShare/bookMark_FF.jsp?title="+encodeURIComponent(title)+"&url="+encodeURIComponent(url);openwindow(url);}function shareToTengxun(title,url){var url="http://v.t.qq.com/share/share.php?title="+encodeURIComponent(title)+"&url="+encodeURIComponent(url);openwindow(url);}
function shareToDouban(title,url){var url="http://www.douban.com/recommend/?title="+encodeURIComponent(title)+"&url="+encodeURIComponent(url);openwindow(url);}function shareToRenren(title,url){var url="http://share.renren.com/share/buttonshare?title="+encodeURIComponent(title)+"&link="+encodeURIComponent(url);openwindow(url);}
function shareToXianguo(title,url){var url="http://www.xianguo.com/service/submitdigg/?title="+encodeURIComponent(title)+"&link="+encodeURIComponent(url);openwindow(url);}function shareToZuosa(title,url){var url="http://zuosa.com/collect/Collect.aspx?t="+encodeURIComponent(title)+"&u="+encodeURIComponent(url);openwindow(url);}function shareToFollow5(title,url){var url="http://www.follow5.com/f5/discuz/sharelogin.jsp?title="+encodeURIComponent(title)+"&url="+encodeURIComponent(url);openwindow(url);}function shareToTongxue(title,url){var url="http://share.tongxue.com/share/buttonshare.php?link="+encodeURIComponent(title)+"&title="+encodeURIComponent(url);openwindow(url);}function shareToKaixin(title,url){var url="http://www.kaixin001.com/repaste/share.php?rtitle="+encodeURIComponent(title)+"&rurl="+encodeURIComponent(url);openwindow(url);}function shareToFanfou(title,url){var url="http://fanfou.com/share.new?t="+encodeURIComponent(title)+"&u="+encodeURIComponent(url);openwindow(url);}function shareToBaidu(title,url){var url="http://cang.baidu.com/do/add?it="+encodeURIComponent(title)+"&iu="+encodeURIComponent(url)+'&fr=ien#nw=1';openwindow(url);}
function shareToTwitter(title,url){var url="http://twitter.com/home?status="+encodeURIComponent(title)+" "+encodeURIComponent(url);openwindow(url);}function shareToFavorite(title,url){if(document.all){window.external.addFavorite(url,title);}else if(window.chrome){alert('Press ctrl+D to bookmark this page.');}else if(window.addPanel){window.sidebar.addPanel(title,url,"");}else{alert('Press ctrl+D to bookmark this page.');}}function openwindow(url){window.open(url,'shareTo','height=300, width=400, toolbar=no, menubar=no, scrollbars=no, resizable=no,location=no, status=no');}