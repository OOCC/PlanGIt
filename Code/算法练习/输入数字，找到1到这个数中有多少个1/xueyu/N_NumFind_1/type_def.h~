#ifndef _TYPE_DEF_H
#define _TYPE_DEF_H

typedef char    CHAR;    
typedef short   SHORT;   
typedef int     INT32;
typedef long    LONG;`

typedef unsigned char   UCHAR;       
typedef unsigned short  USHROT;
typedef unsigned int    UINT32;
typedef unsigned long   ULONG;

#define  ERR 1
#define  OK  0


#endif
